<!DOCTYPE HTML>
<html>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta http-equiv='Content-Type' content='text/html;charset=UTF-8'/>
		<link rel = "stylesheet" href = "css/index.css">
		<script
            	id='sap-ui-bootstrap'
            	type='text/javascript'
            	src='resources/sap-ui-core.js'
            	data-sap-ui-xx-bindingSyntax="complex"
            	data-sap-ui-theme='sap_bluecrystal'
            	data-sap-ui-libs='sap.m, sap.ui.layout,sap.ui.commons'
            	data-sap-ui-compatVersion="1.18">
	    </script>
		<!-- only load the mobile lib "sap.m" and the "sap_bluecrystal" theme -->

		<script>
				sap.ui.localResources("smpdemo");
				var app = new sap.m.App({initialPage:"idRegister1"});
				var page1 = sap.ui.view({id:"idRegister1", viewName:"smpdemo.Register", type:sap.ui.core.mvc.ViewType.JS});
				var page2 = sap.ui.view({id:"idLanding", viewName:"smpdemo.Landing", type:sap.ui.core.mvc.ViewType.JS});
				var page3 = sap.ui.view({id:"idAccountApp", viewName:"smpdemo.AccountApp", type:sap.ui.core.mvc.ViewType.JS});
				var page4 = sap.ui.view({id:"idLeadApp", viewName:"smpdemo.LeadApp", type:sap.ui.core.mvc.ViewType.JS});
				var page5 = sap.ui.view({id:"idCaseApp", viewName:"smpdemo.CaseApp", type:sap.ui.core.mvc.ViewType.JS});
				var page6 = sap.ui.view({id:"idContactApp", viewName:"smpdemo.ContactApp", type:sap.ui.core.mvc.ViewType.JS});
				var page7 = sap.ui.view({id:"idOpportunityApp", viewName:"smpdemo.OpportunityApp", type:sap.ui.core.mvc.ViewType.JS});
				app.addPage(page1).addPage(page2).addPage(page3).addPage(page4).addPage(page5).addPage(page6).addPage(page7);
				app.placeAt("content");
		</script>

	</head>
	<body class="sapUiBody" role="application">
		<div id="content"></div>
	</body>
</html>